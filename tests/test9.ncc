routine main() is
    arr: array[5]integer is [5, -6, 0, 12, 2]
    // sorted(arr) returns a sorted copy of arr, arr does not change
    sorted_arr: array[5]integer is sorted(arr)
    assert sorted_arr, [-6, 0, 2, 5, 12] // OK
    // reversed(arr) returns reversed copy of arr, arr does not change
    reversed_arr: array[5]integer is reversed(sorted_arr)
    assert reversed_arr, [12, 5, 2, 0, -6] // OK
end