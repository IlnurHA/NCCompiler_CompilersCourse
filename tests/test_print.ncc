routine main() is 
    // integer printing
    var n: integer
    n := 10
    print(n)
    // real printing
    var m: real
    m := 3.5
    print(m)
    // boolean printing
    var b: boolean
    b := true
    print(b)
    // expression printing
    print(3 + 6 * 4)
    // expressions printing
    print(n, m, 2+2, b)
end